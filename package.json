{
  "name": "rest-api-kit",
  "version": "0.0.53",
  "description": "Enterprise-grade TypeScript REST API management library for React and React Native with advanced caching, state management, and performance optimization",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,json,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,json,md}",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm test",
    "release": "npm version patch && npm publish",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "rest-api",
    "api-client",
    "http-client",
    "react",
    "react-native",
    "typescript",
    "cache-management",
    "state-management",
    "hooks",
    "fetch",
    "axios-alternative",
    "enterprise",
    "performance",
    "mobile",
    "web",
    "offline-support",
    "interceptors",
    "middleware",
    "retry-logic",
    "devtools"
  ],
  "author": {
    "name": "rxyce",
    "email": "support@rest-api-kit.dev",
    "url": "https://github.com/naries"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/naries/rest-api-kit.git"
  },
  "bugs": {
    "url": "https://github.com/naries/rest-api-kit/issues"
  },
  "homepage": "https://github.com/naries/rest-api-kit#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^3.2.5",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-test-renderer": "^17.0.0",
    "ts-jest": "^29.1.3",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/naries"
  },
  "publishConfig": {
    "access": "public"
  }
}